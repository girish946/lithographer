{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devPath": "../src",
    "distDir": "../src",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "lithographer",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "fs": {
        "all": false
      },
      "dialog": {
        "all": false,
        "open": true,
        "save": true
      },
      "shell": {
        "all": false,
        "execute": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.lithographer.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "cli": {
      "description": "",
      "longDescription": "",
      "beforeHelp": "",
      "afterHelp": "",
      "args": [],
      "subcommands": {
        "clone": {
          "args": [
            {
              "name": "file",
              "short": "f",
              "takesValue": true,
              "multiple": false
            },
            {
              "name": "disk",
              "short": "d",
              "takesValue": true,
              "multiple": false
            }
          ]
        },
        "flash": {
          "args": [
            {
              "name": "file",
              "short": "f",
              "takesValue": true,
              "multiple": false
            },
            {
              "name": "disk",
              "short": "d",
              "takesValue": true,
              "multiple": false
            }
          ]
        }
      }
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": false,
        "title": "lithographer",
        "width": 1200,
        "height": 600
      }
    ]
  }
}
